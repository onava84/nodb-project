(this.props.id);\n  };\n\n  render() {\n    return (\n      <div className=\"player-card\">\n        <img src={this.props.image} className=\"player-image\" />\n        <h4 className=\"player-name\">{this.props.name}</h4>\n        <p>Position: {this.props.position}</p>\n        <p>Value: {this.props.value} M</p>\n        <p>Team: {this.props.team}</p>\n        <button onClick={e => this.props.handleAddToTeam(this.props.id)}>\n          ADD TO MY TEAM\n        </button>\n        {this.state.editing ? (\n          <div className=\"update-fields\">\n            <p className=\"update-info-card\">Update info card</p>\n            <input\n              name=\"name\"\n              value={this.state.name}\n              placeholder=\"Name\"\n              className=\"update-field\"\n              onChange={this.handleInput}\n            />\n            <input\n              name=\"position\"\n              value={this.state.position}\n              placeholder=\"Position\"\n              className=\"update-field\"\n              onChange={this.handleInput}\n            />\n            <input\n              name=\"value\"\n              value={this.state.value}\n              placeholder=\"Value\"\n              className=\"update-field\"\n              onChange={this.handleInput}\n            />\n            <input\n              name=\"team\"\n              value={this.state.team}\n              placeholder=\"Team\"\n              className=\"update-field\"\n              onChange={this.handleInput}\n            />\n            <input\n              name=\"image\"\n              value={this.state.image}\n              placeholder=\"Image\"\n              className=\"update-field\"\n              onChange={this.handleInput}\n            />\n            <p className=\"fields-required\">\n              {this.state.fieldEmpty\n                ? \"You should fill at least 1 field\"\n                : null}\n            </p>\n          </div>\n        ) : (\n          \"\"\n        )}\n        <button\n          onClick={e => this.toggleEditing(e)}\n          className={this.state.editing ? \"cancel\" : \"\"}\n        >\n          {this.state.editing ? \"CANCEL\" : \"EDIT PLAYER INFO\"}\n        </button>\n        {this.state.editing ? (\n          <button\n            className=\"save-button\"\n            onClick={e => this.handleEditing(this.props.id)}\n          >\n            SAVE\n          </button>\n        ) : null}\n        <button onClick={this.handleClickDelete}>DELETE PLAYER</button>\n      </div>\n    );\n  }\n}\n\nexport default Player;\n","/Users/oscarnava/Desktop/devmountain/my-soccer-team/my-soccer-team/src/components/newPlayer/NewPlayer.js",[],"/Users/oscarnava/Desktop/devmountain/my-soccer-team/my-soccer-team/src/components/team/team.js",["56"],"/Users/oscarnava/Desktop/devmountain/my-soccer-team/my-soccer-team/src/components/team/Team.js",["57"],"import React, { Component } from \"react\";\nimport Teammate from \"../../teammates/Teammate\";\nimport \"./team.css\";\n\nclass Team extends Component {\n  constructor(props) {\n    super(props);\n  }\n\n  render() {\n    const teamMap = this.props.teamToBuild.map(e => {\n      return (\n        <Teammate\n          playerName={e.name}\n          handleRemoveTeam={this.props.handleRemoveTeam}\n          playerId={e.id}\n        />\n      );\n    });\n    return (\n      <div className=\"team-block\">\n        <h2>My Team</h2>\n        <div>{teamMap}</div>\n        <h3 className=\"total-market-value\">Total Market Value</h3>\n        <h4>${this.props.totalValue} M</h4>\n      </div>\n    );\n  }\n}\n\nexport default Team;\n","/Users/oscarnava/Desktop/devmountain/my-soccer-team/my-soccer-team/src/teammates/Teammate.js",[],"/Users/oscarnava/Desktop/devmountain/my-soccer-team/my-soccer-team/src/components/filter/filter.js",["58"],"/Users/oscarnava/Desktop/devmountain/my-soccer-team/my-soccer-team/src/components/filter/Filter.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61"